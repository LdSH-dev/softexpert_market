{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'SellScreen',\n  data() {\n    return {\n      search: '',\n      searchResults: [],\n      products: []\n    };\n  },\n  methods: {\n    calculateTax(price) {\n      const taxRate = 0.1; // 10% tax rate\n      return (price * taxRate).toFixed(2);\n    },\n    calculateSubtotal() {\n      let subtotal = 0;\n      this.products.forEach(product => {\n        subtotal += product.price * product.quantity;\n      });\n      return subtotal.toFixed(2);\n    },\n    calculateTotalTax() {\n      let totalTax = 0;\n      this.products.forEach(product => {\n        totalTax += this.calculateTax(product.price) * product.quantity;\n      });\n      return totalTax.toFixed(2);\n    },\n    calculateGrandTotal() {\n      const subtotal = parseFloat(this.calculateSubtotal());\n      const totalTax = parseFloat(this.calculateTotalTax());\n      return (subtotal + totalTax).toFixed(2);\n    },\n    addProduct() {\n      if (this.search) {\n        const product = {\n          id: Date.now(),\n          name: this.search,\n          category: 'Category',\n          price: 0,\n          quantity: 0\n        };\n        this.products.push(product);\n        this.search = '';\n        this.searchResults = [];\n      }\n    },\n    addProductFromSearch(product) {\n      this.products.push(product);\n      this.search = '';\n      this.searchResults = [];\n    },\n    deleteProduct(index) {\n      this.products.splice(index, 1);\n    }\n  },\n  watch: {\n    search: function () {\n      this.searchResults = this.getSearchResults();\n    }\n  },\n  computed: {\n    filteredProducts() {\n      // Aqui você pode realizar a lógica de busca nos produtos fictícios\n      const searchTerm = this.search.toLowerCase();\n      return this.fictitiousProducts.filter(product => product.name.toLowerCase().includes(searchTerm));\n    },\n    fictitiousProducts() {\n      // Aqui você pode retornar os produtos fictícios\n      return [{\n        id: 1,\n        name: 'Product 1',\n        category: 'Category 1',\n        price: 10,\n        quantity: 0\n      }, {\n        id: 2,\n        name: 'Product 2',\n        category: 'Category 2',\n        price: 20,\n        quantity: 0\n      }, {\n        id: 3,\n        name: 'Product 3',\n        category: 'Category 1',\n        price: 30,\n        quantity: 0\n      }];\n    }\n  }\n};","map":{"version":3,"names":["name","data","search","searchResults","products","methods","calculateTax","price","taxRate","toFixed","calculateSubtotal","subtotal","forEach","product","quantity","calculateTotalTax","totalTax","calculateGrandTotal","parseFloat","addProduct","id","Date","now","category","push","addProductFromSearch","deleteProduct","index","splice","watch","getSearchResults","computed","filteredProducts","searchTerm","toLowerCase","fictitiousProducts","filter","includes"],"sources":["/home/ldsh-dev/projects/softexpert/resources/src/components/SellScreen.vue"],"sourcesContent":["<template>\r\n  <div id=\"sell\">\r\n    <h1 class=\"sell-title\">Sales</h1>\r\n    <input type=\"text\" v-model=\"search\" @keyup.enter=\"addProduct\" placeholder=\"Search product\">\r\n\r\n    <ul class=\"search-results\">\r\n      <li v-for=\"product in searchResults\" :key=\"product.id\" @click=\"addProductFromSearch(product)\">\r\n        {{ product.name }}\r\n      </li>\r\n    </ul>\r\n\r\n    <table class=\"product-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Quantity</th>\r\n          <th>Product</th>\r\n          <th>Category</th>\r\n          <th>Price ($)</th>\r\n          <th>Tax ($)</th>\r\n          <th></th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(product, index) in products\" :key=\"index\">\r\n          <td>\r\n            <input type=\"number\" v-model=\"product.quantity\" min=\"0\" style=\"width: 50px;\">\r\n          </td>\r\n          <td>{{ product.name }}</td>\r\n          <td>{{ product.category }}</td>\r\n          <td>${{ product.price }}</td>\r\n          <td>${{ calculateTax(product.price) }}</td>\r\n          <td>\r\n            <button class=\"delete-button\" @click=\"deleteProduct(index)\">X</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <div class=\"total\">\r\n      Subtotal: ${{ calculateSubtotal() }}\r\n    </div>\r\n    <div class=\"tax\">\r\n      Total Tax: ${{ calculateTotalTax() }}\r\n    </div>\r\n    <div class=\"grand-total\">\r\n      Grand Total: ${{ calculateGrandTotal() }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'SellScreen',\r\n  data() {\r\n    return {\r\n      search: '',\r\n      searchResults: [],\r\n      products: []\r\n    };\r\n  },\r\n  methods: {\r\n    calculateTax(price) {\r\n      const taxRate = 0.1; // 10% tax rate\r\n      return (price * taxRate).toFixed(2);\r\n    },\r\n    calculateSubtotal() {\r\n      let subtotal = 0;\r\n      this.products.forEach((product) => {\r\n        subtotal += product.price * product.quantity;\r\n      });\r\n      return subtotal.toFixed(2);\r\n    },\r\n    calculateTotalTax() {\r\n      let totalTax = 0;\r\n      this.products.forEach((product) => {\r\n        totalTax += this.calculateTax(product.price) * product.quantity;\r\n      });\r\n      return totalTax.toFixed(2);\r\n    },\r\n    calculateGrandTotal() {\r\n      const subtotal = parseFloat(this.calculateSubtotal());\r\n      const totalTax = parseFloat(this.calculateTotalTax());\r\n      return (subtotal + totalTax).toFixed(2);\r\n    },\r\n    addProduct() {\r\n      if (this.search) {\r\n        const product = {\r\n          id: Date.now(),\r\n          name: this.search,\r\n          category: 'Category',\r\n          price: 0,\r\n          quantity: 0\r\n        };\r\n        this.products.push(product);\r\n        this.search = '';\r\n        this.searchResults = [];\r\n      }\r\n    },\r\n    addProductFromSearch(product) {\r\n      this.products.push(product);\r\n      this.search = '';\r\n      this.searchResults = [];\r\n    },\r\n    deleteProduct(index) {\r\n      this.products.splice(index, 1);\r\n    }\r\n  },\r\n  watch: {\r\n    search: function () {\r\n      this.searchResults = this.getSearchResults();\r\n    }\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      // Aqui você pode realizar a lógica de busca nos produtos fictícios\r\n      const searchTerm = this.search.toLowerCase();\r\n      return this.fictitiousProducts.filter((product) =>\r\n        product.name.toLowerCase().includes(searchTerm)\r\n      );\r\n    },\r\n    fictitiousProducts() {\r\n      // Aqui você pode retornar os produtos fictícios\r\n      return [\r\n        { id: 1, name: 'Product 1', category: 'Category 1', price: 10, quantity: 0 },\r\n        { id: 2, name: 'Product 2', category: 'Category 2', price: 20, quantity: 0 },\r\n        { id: 3, name: 'Product 3', category: 'Category 1', price: 30, quantity: 0 }\r\n      ];\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#sell {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.sell-title {\r\n  font-size: 28px;\r\n  margin-bottom: 20px;\r\n  color: #333;\r\n}\r\n\r\n.product-table {\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n}\r\n\r\n.product-table th,\r\n.product-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.product-table th {\r\n  background-color: #f2f2f2;\r\n  text-align: left;\r\n}\r\n\r\n.total {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  margin-top: 20px;\r\n}\r\n\r\n.tax {\r\n  font-size: 16px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.grand-total {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-top: 10px;\r\n}\r\n\r\n.delete-button {\r\n  background-color: #f44336;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.search-results {\r\n  list-style-type: none;\r\n  padding: 0;\r\n  margin-top: 10px;\r\n}\r\n\r\n.search-results li {\r\n  cursor: pointer;\r\n  padding: 5px;\r\n}\r\n\r\n.search-results li:hover {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":";AAmDA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,YAAYA,CAACC,KAAK,EAAE;MAClB,MAAMC,OAAM,GAAI,GAAG,EAAE;MACrB,OAAO,CAACD,KAAI,GAAIC,OAAO,EAAEC,OAAO,CAAC,CAAC,CAAC;IACrC,CAAC;IACDC,iBAAiBA,CAAA,EAAG;MAClB,IAAIC,QAAO,GAAI,CAAC;MAChB,IAAI,CAACP,QAAQ,CAACQ,OAAO,CAAEC,OAAO,IAAK;QACjCF,QAAO,IAAKE,OAAO,CAACN,KAAI,GAAIM,OAAO,CAACC,QAAQ;MAC9C,CAAC,CAAC;MACF,OAAOH,QAAQ,CAACF,OAAO,CAAC,CAAC,CAAC;IAC5B,CAAC;IACDM,iBAAiBA,CAAA,EAAG;MAClB,IAAIC,QAAO,GAAI,CAAC;MAChB,IAAI,CAACZ,QAAQ,CAACQ,OAAO,CAAEC,OAAO,IAAK;QACjCG,QAAO,IAAK,IAAI,CAACV,YAAY,CAACO,OAAO,CAACN,KAAK,IAAIM,OAAO,CAACC,QAAQ;MACjE,CAAC,CAAC;MACF,OAAOE,QAAQ,CAACP,OAAO,CAAC,CAAC,CAAC;IAC5B,CAAC;IACDQ,mBAAmBA,CAAA,EAAG;MACpB,MAAMN,QAAO,GAAIO,UAAU,CAAC,IAAI,CAACR,iBAAiB,CAAC,CAAC,CAAC;MACrD,MAAMM,QAAO,GAAIE,UAAU,CAAC,IAAI,CAACH,iBAAiB,CAAC,CAAC,CAAC;MACrD,OAAO,CAACJ,QAAO,GAAIK,QAAQ,EAAEP,OAAO,CAAC,CAAC,CAAC;IACzC,CAAC;IACDU,UAAUA,CAAA,EAAG;MACX,IAAI,IAAI,CAACjB,MAAM,EAAE;QACf,MAAMW,OAAM,GAAI;UACdO,EAAE,EAAEC,IAAI,CAACC,GAAG,CAAC,CAAC;UACdtB,IAAI,EAAE,IAAI,CAACE,MAAM;UACjBqB,QAAQ,EAAE,UAAU;UACpBhB,KAAK,EAAE,CAAC;UACRO,QAAQ,EAAE;QACZ,CAAC;QACD,IAAI,CAACV,QAAQ,CAACoB,IAAI,CAACX,OAAO,CAAC;QAC3B,IAAI,CAACX,MAAK,GAAI,EAAE;QAChB,IAAI,CAACC,aAAY,GAAI,EAAE;MACzB;IACF,CAAC;IACDsB,oBAAoBA,CAACZ,OAAO,EAAE;MAC5B,IAAI,CAACT,QAAQ,CAACoB,IAAI,CAACX,OAAO,CAAC;MAC3B,IAAI,CAACX,MAAK,GAAI,EAAE;MAChB,IAAI,CAACC,aAAY,GAAI,EAAE;IACzB,CAAC;IACDuB,aAAaA,CAACC,KAAK,EAAE;MACnB,IAAI,CAACvB,QAAQ,CAACwB,MAAM,CAACD,KAAK,EAAE,CAAC,CAAC;IAChC;EACF,CAAC;EACDE,KAAK,EAAE;IACL3B,MAAM,EAAE,SAAAA,CAAA,EAAY;MAClB,IAAI,CAACC,aAAY,GAAI,IAAI,CAAC2B,gBAAgB,CAAC,CAAC;IAC9C;EACF,CAAC;EACDC,QAAQ,EAAE;IACRC,gBAAgBA,CAAA,EAAG;MACjB;MACA,MAAMC,UAAS,