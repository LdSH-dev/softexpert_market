{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  name: 'SellScreen',\n  data() {\n    return {\n      search: '',\n      searchResults: [],\n      products: [],\n      dataList: []\n    };\n  },\n  mounted() {\n    this.fetchData();\n  },\n  methods: {\n    fetchData() {\n      axios.get('http://localhost:8000/product').then(response => {\n        if (response.data['success']) {\n          this.dataList = response.data['data'];\n        } else {\n          this.dataList = [];\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n      axios.get('http://localhost:8000/category').then(response => {\n        if (response.data['success']) {\n          this.categories = response.data['data'];\n        } else {\n          this.categories = [];\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    updateQuantitySelected(event, product) {\n      const newValue = event.target.value;\n      if (/^\\d*$/.test(newValue)) {\n        product.quantity_selected = newValue;\n      } else {\n        // Aqui você pode tratar o caso em que o valor inserido não é um número válido\n        // Por exemplo, exibir uma mensagem de erro ou reverter para o valor anterior\n      }\n    },\n    calculateTax(price, fee) {\n      return (price * fee).toFixed(2);\n    },\n    calculateSubtotal() {\n      let subtotal = 0;\n      this.products.forEach(product => {\n        subtotal += product.price * product.quantity;\n      });\n      return subtotal.toFixed(2);\n    },\n    calculateTotalTax() {\n      let totalTax = 0;\n      this.products.forEach(product => {\n        totalTax += this.calculateTax(product.price, product.category.fee) * product.quantity;\n        console.log(totalTax);\n      });\n      return totalTax.toFixed(2);\n    },\n    getSearchResults() {\n      if (this.search) {\n        const searchTerm = this.search.toLowerCase();\n        return this.dataList.filter(product => product.name.toLowerCase().includes(searchTerm));\n      } else {\n        return [];\n      }\n    },\n    calculateGrandTotal() {\n      const subtotal = parseFloat(this.calculateSubtotal());\n      const totalTax = parseFloat(this.calculateTotalTax());\n      return (subtotal + totalTax).toFixed(2);\n    },\n    addProduct() {\n      if (this.search) {\n        const product = {\n          id: Date.now(),\n          name: this.search,\n          category: 'Category',\n          price: 0,\n          quantity: 0\n        };\n        this.products.push(product);\n        this.search = '';\n        this.searchResults = [];\n      }\n    },\n    addProductFromSearch(product) {\n      this.products.push(product);\n      this.search = '';\n      this.searchResults = [];\n    },\n    deleteProduct(index) {\n      this.products.splice(index, 1);\n    },\n    sellProducts() {\n      const requestData = {\n        total_price: this.calculateGrandTotal(),\n        total_fee: this.calculateTotalTax(),\n        items: this.products.map(product => ({\n          id: product.id,\n          name: product.name,\n          category: product.category,\n          price: product.price,\n          quantity: product.quantity,\n          quantity_selected: product.quantity_selected\n        }))\n      };\n      axios.post('http://localhost:8000/sale/create', requestData).then(response => {\n        if (response.data['success']) {\n          this.fetchData();\n        } else {\n          alert(response.data['error']);\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  watch: {\n    search: function () {\n      this.searchResults = this.getSearchResults();\n    }\n  },\n  computed: {\n    filteredProducts() {\n      if (this.search) {\n        const searchTerm = this.search.toLowerCase();\n        return this.dataList.filter(product => product.name.toLowerCase().includes(searchTerm));\n      } else {\n        return [];\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","data","search","searchResults","products","dataList","mounted","fetchData","methods","get","then","response","catch","error","console","categories","updateQuantitySelected","event","product","newValue","target","value","test","quantity_selected","calculateTax","price","fee","toFixed","calculateSubtotal","subtotal","forEach","quantity","calculateTotalTax","totalTax","category","log","getSearchResults","searchTerm","toLowerCase","filter","includes","calculateGrandTotal","parseFloat","addProduct","id","Date","now","push","addProductFromSearch","deleteProduct","index","splice","sellProducts","requestData","total_price","total_fee","items","map","post","alert","watch","computed","filteredProducts"],"sources":["/home/ldsh-dev/projects/softexpert/resources/src/components/SellScreen.vue"],"sourcesContent":["<template>\r\n  <div id=\"sell\">\r\n    <h1 class=\"sell-title\">Sales</h1>\r\n    <input type=\"text\" v-model=\"search\" @keyup.enter=\"addProduct\" placeholder=\"Search product\">\r\n\r\n    <ul class=\"search-results\">\r\n      <li v-for=\"product in searchResults\" :key=\"product.id\" @click=\"addProductFromSearch(product)\">\r\n        {{ product.name }}\r\n      </li>\r\n    </ul>\r\n\r\n    <table class=\"product-table\">\r\n      <thead>\r\n        <tr>\r\n          <th>Quantity</th>\r\n          <th>Product</th>\r\n          <th>Category</th>\r\n          <th>Price ($)</th>\r\n          <th>Tax ($)</th>\r\n          <th></th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"(product, index) in products\" :key=\"index\">\r\n          <td>\r\n            <input type=\"number\" v-model=\"product.quantity\" min=\"1\" :max=\"product.quantity\" style=\"width: 50px;\" v-numbers-only @input=\"(event) => updateQuantitySelected(event, product)\">\r\n          </td>\r\n          <td>{{ product.name }}</td>\r\n          <td>{{ product.category.name }}</td>\r\n          <td>${{ product.price }}</td>\r\n          <td>${{ calculateTax(product.price, product.category.fee) }}</td>\r\n          <td>\r\n            <button class=\"delete-button\" @click=\"deleteProduct(index)\">X</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <div class=\"total\">\r\n      Subtotal: ${{ calculateSubtotal() }}\r\n    </div>\r\n    <div class=\"tax\">\r\n      Total Tax: ${{ calculateTotalTax() }}\r\n    </div>\r\n    <div class=\"grand-total\">\r\n      Grand Total: ${{ calculateGrandTotal() }}\r\n    </div>\r\n\r\n    <button class=\"sale-button\" @click=\"sellProducts()\">Confirm Sale</button>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'SellScreen',\r\n  data() {\r\n    return {\r\n      search: '',\r\n      searchResults: [],\r\n      products: [],\r\n      dataList: []\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchData();\r\n  },\r\n  methods: {\r\n    fetchData() {\r\n      axios.get('http://localhost:8000/product')\r\n        .then(response => {\r\n          if (response.data['success'])\r\n          {\r\n            this.dataList = response.data['data'];\r\n          }\r\n          else\r\n          {\r\n            this.dataList = [];\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n\r\n        axios.get('http://localhost:8000/category')\r\n        .then(response => {\r\n          if (response.data['success'])\r\n          {\r\n            this.categories = response.data['data'];\r\n          }\r\n          else\r\n          {\r\n            this.categories = [];\r\n          }\r\n            \r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    updateQuantitySelected(event, product) {\r\n    const newValue = event.target.value;\r\n\r\n    if (/^\\d*$/.test(newValue)) {\r\n      product.quantity_selected = newValue;\r\n    } else {\r\n      // Aqui você pode tratar o caso em que o valor inserido não é um número válido\r\n      // Por exemplo, exibir uma mensagem de erro ou reverter para o valor anterior\r\n    }\r\n  },\r\n    calculateTax(price, fee) {\r\n      return (price * fee).toFixed(2);\r\n    },\r\n    calculateSubtotal() {\r\n      let subtotal = 0;\r\n      this.products.forEach((product) => {\r\n        subtotal += product.price * product.quantity;\r\n      });\r\n      return subtotal.toFixed(2);\r\n    },\r\n    calculateTotalTax() {\r\n      let totalTax = 0;\r\n      this.products.forEach((product) => {\r\n        totalTax += this.calculateTax(product.price, product.category.fee) * product.quantity;\r\n        console.log(totalTax);\r\n      });\r\n      return totalTax.toFixed(2);\r\n    },\r\n    getSearchResults() {\r\n      if (this.search) {\r\n        const searchTerm = this.search.toLowerCase();\r\n        return this.dataList.filter((product) =>\r\n          product.name.toLowerCase().includes(searchTerm)\r\n        );\r\n      } else {\r\n        return [];\r\n      }\r\n    },\r\n    calculateGrandTotal() {\r\n      const subtotal = parseFloat(this.calculateSubtotal());\r\n      const totalTax = parseFloat(this.calculateTotalTax());\r\n      return (subtotal + totalTax).toFixed(2);\r\n    },\r\n    addProduct() {\r\n      if (this.search) {\r\n        const product = {\r\n          id: Date.now(),\r\n          name: this.search,\r\n          category: 'Category',\r\n          price: 0,\r\n          quantity: 0\r\n        };\r\n        this.products.push(product);\r\n        this.search = '';\r\n        this.searchResults = [];\r\n      }\r\n    },\r\n    addProductFromSearch(product) {\r\n      this.products.push(product);\r\n      this.search = '';\r\n      this.searchResults = [];\r\n    },\r\n    deleteProduct(index) {\r\n      this.products.splice(index, 1);\r\n    },\r\n    sellProducts() {\r\n      const requestData = {\r\n        total_price: this.calculateGrandTotal(),\r\n        total_fee: this.calculateTotalTax(),\r\n        items: this.products.map((product) => ({\r\n          id: product.id,\r\n          name: product.name,\r\n          category: product.category,\r\n          price: product.price,\r\n          quantity: product.quantity,\r\n          quantity_selected: product.quantity_selected\r\n        }))\r\n      };\r\n    \r\n      axios\r\n        .post('http://localhost:8000/sale/create', requestData)\r\n        .then(response => {\r\n          if (response.data['success']) {\r\n            this.fetchData();\r\n          } else {\r\n            alert(response.data['error']);\r\n          }\r\n        })\r\n        .catch(error => {\r\n          console.error(error);\r\n        });\r\n    }\r\n\r\n  },\r\n  watch: {\r\n    search: function () {\r\n      this.searchResults = this.getSearchResults();\r\n    }\r\n  },\r\n  computed: {\r\n    filteredProducts() {\r\n      if (this.search) {\r\n        const searchTerm = this.search.toLowerCase();\r\n        return this.dataList.filter((product) =>\r\n          product.name.toLowerCase().includes(searchTerm)\r\n        );\r\n      } else {\r\n        return [];\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#sell {\r\n  margin-top: 20px;\r\n  text-align: center;\r\n}\r\n\r\n.sell-title {\r\n  font-size: 28px;\r\n  margin-bottom: 20px;\r\n  color: #333;\r\n}\r\n\r\n.product-table {\r\n  margin-top: 20px;\r\n  margin-bottom: 20px;\r\n  border-collapse: collapse;\r\n  width: 100%;\r\n}\r\n\r\n.product-table th,\r\n.product-table td {\r\n  border: 1px solid #ddd;\r\n  padding: 8px;\r\n}\r\n\r\n.product-table th {\r\n  background-color: #f2f2f2;\r\n  text-align: left;\r\n}\r\n\r\n.total {\r\n  font-size: 20px;\r\n  font-weight: bold;\r\n  margin-top: 20px;\r\n}\r\n\r\n.tax {\r\n  font-size: 16px;\r\n  margin-top: 10px;\r\n}\r\n\r\n.grand-total {\r\n  font-size: 24px;\r\n  font-weight: bold;\r\n  margin-top: 10px;\r\n}\r\n\r\n.delete-button {\r\n  background-color: #f44336;\r\n  color: white;\r\n  border: none;\r\n  padding: 5px 10px;\r\n  cursor: pointer;\r\n}\r\n\r\n.search-results {\r\n  list-style-type: none;\r\n  padding: 0;\r\n  margin-top: 10px;\r\n}\r\n\r\n.search-results li {\r\n  cursor: pointer;\r\n  padding: 5px;\r\n}\r\n\r\n.search-results li:hover {\r\n  background-color: #f2f2f2;\r\n}\r\n</style>\r\n"],"mappings":";AAsDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAS,CAAC,CAAC;EAClB,CAAC;EACDC,OAAO,EAAE;IACPD,SAASA,CAAA,EAAG;MACVR,KAAK,CAACU,GAAG,CAAC,+BAA+B,EACtCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACV,IAAI,CAAC,SAAS,CAAC,EAC5B;UACE,IAAI,CAACI,QAAO,GAAIM,QAAQ,CAACV,IAAI,CAAC,MAAM,CAAC;QACvC,OAEA;UACE,IAAI,CAACI,QAAO,GAAI,EAAE;QACpB;MACF,CAAC,EACAO,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;MAEFd,KAAK,CAACU,GAAG,CAAC,gCAAgC,EACzCC,IAAI,CAACC,QAAO,IAAK;QAChB,IAAIA,QAAQ,CAACV,IAAI,CAAC,SAAS,CAAC,EAC5B;UACE,IAAI,CAACc,UAAS,GAAIJ,QAAQ,CAACV,IAAI,CAAC,MAAM,CAAC;QACzC,OAEA;UACE,IAAI,CAACc,UAAS,GAAI,EAAE;QACtB;MAEF,CAAC,EACAH,KAAK,CAACC,KAAI,IAAK;QACdC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDG,sBAAsBA,CAACC,KAAK,EAAEC,OAAO,EAAE;MACvC,MAAMC,QAAO,GAAIF,KAAK,CAACG,MAAM,CAACC,KAAK;MAEnC,IAAI,OAAO,CAACC,IAAI,CAACH,QAAQ,CAAC,EAAE;QAC1BD,OAAO,CAACK,iBAAgB,GAAIJ,QAAQ;MACtC,OAAO;QACL;QACA;MAAA;IAEJ,CAAC;IACCK,YAAYA,CAACC,KAAK,EAAEC,GAAG,EAAE;MACvB,OAAO,CAACD,KAAI,GAAIC,GAAG,EAAEC,OAAO,CAAC,CAAC,CAAC;IACjC,CAAC;IACDC,iBAAiBA,CAAA,EAAG;MAClB,IAAIC,QAAO,GAAI,CAAC;MAChB,IAAI,CAACzB,QAAQ,CAAC0B,OAAO,CAAEZ,OAAO,IAAK;QACjCW,QAAO,IAAKX,OAAO,CAACO,KAAI,GAAIP,OAAO,CAACa,QAAQ;MAC9C,CAAC,CAAC;MACF,OAAOF,QAAQ,CAACF,OAAO,CAAC,CAAC,CAAC;IAC5B,CAAC;IACDK,iBAAiBA,CAAA,EAAG;MAClB,IAAIC,QAAO,GAAI,CAAC;MAChB,IAAI,CAAC7B,QAAQ,CAAC0B,OAAO,CAAEZ,OAAO,IAAK;QACjCe,QAAO,IAAK,IAAI,CAACT,YAAY,CAAC